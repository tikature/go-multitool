{
    "version": 2,
    "builds": [
        {
            "src": "api/index/index.go",
            "use": "@vercel/go"
        },
        {
            "src": "api/calculator/calculator.go",
            "use": "@vercel/go"
        },
        {
            "src": "api/games/games.go",
            "use": "@vercel/go"
        },
        {
            "src": "api/textutils/textutils.go",
            "use": "@vercel/go"
        },
        {
            "src": "api/textformatter/textformatter.go",
            "use": "@vercel/go"
        },
        {
            "src": "api/unitconverter/unitconverter.go",
            "use": "@vercel/go"
        },
        {
            "src": "api/quotes/quotes.go",
            "use": "@vercel/go"
        },
        {
            "src": "api/bmi/bmi.go",
            "use": "@vercel/go"
        },
        {
            "src": "api/num2words/num2words.go",
            "use": "@vercel/go"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/api/index/index.go"
        },
        {
            "src": "/calculator",
            "dest": "/api/calculator/calculator.go"
        },
        {
            "src": "/games",
            "dest": "/api/games/games.go"
        },
        {
            "src": "/textutils",
            "dest": "/api/textutils/textutils.go"
        },
        {
            "src": "/textformatter",
            "dest": "/api/textformatter/textformatter.go"
        },
        {
            "src": "/unitconverter",
            "dest": "/api/unitconverter/unitconverter.go"
        },
        {
            "src": "/quotes",
            "dest": "/api/quotes/quotes.go"
        },
        {
            "src": "/bmi",
            "dest": "/api/bmi/bmi.go"
        },
        {
            "src": "/num2words",
            "dest": "/api/num2words/num2words.go"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        }
    ]
}